@article{Boguslawski_2016c,
	abstract = {{The character of the 21Ag− for different molecular structures can be found in Fig. 3 and should be compared to Fig. 4 of the original article. For the DFT-optimized structures, the contribution from the doubly excited configuration HOMO2 → LUMO2 increases to approximately 50\\%--60\\%, while both singly excited configurations constitute 15\\%--25\\%. For longer polyene chain lengths, the contribution from the doubly excited configuration decreases. One major difference between the corrected results and the originally published data can be found for the character of the vertically excited states using the DMRG-optimized structures. Originally, the character of the first dark state was dominated by the HOMO → LUMO+1 excitation for longer polyene chain lengths, followed by the HOMO2 → LUMO2 excitation. With the corrected implementation, the vertically excited 21Ag− state becomes dominated by the HOMO−1 → LUMO excitation (gradually increasing from 35\\%--65\\%), while the HOMO2 → LUMO2 contribution gradually decreases from approximately 50\\% for C10H12 to less than 10\\% for C14H16. Similar to the original results, the picture changes completely when the molecular structure is allowed to relax. With the correct implementation, the contribution of the HOMO2 → LUMO2 excitation increases from 60 to roughly 70\\% compared to the previously reported data, while both singly excited configurations contribute approximately 10\\% to the first dark state of longer polyene chain lengths. Thus, the description of the 21Ag− state improves using the corrected implementation of EOM-pCCD+S, which explains the lower excitation energies for the first dark state compared to the original results.}},
	author = {Boguslawski, Katharina},
	doi = {10.1063/1.5006124},
	eprint = {https://pubs.aip.org/aip/jcp/article-pdf/doi/10.1063/1.5006124/15535869/139901\_1\_online.pdf},
	issn = {0021-9606},
	journal = {J. Chem. Phys.},
	month = {10},
	note = {139901},
	number = {13},
	title = {{Erratum: ``Targeting excited states in all-trans polyenes with electron-pair states'' [J. Chem. Phys. 145, 234105 (2016)]}},
	url = {https://doi.org/10.1063/1.5006124},
	volume = {147},
	year = {2017},
